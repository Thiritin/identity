{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "doctrine/dbal": "^3.5",
        "fideloper/proxy": "^4.4",
        "filament/filament": "^2.16",
        "filament/forms": "^2.16",
        "filament/notifications": "^2.16",
        "filament/spatie-laravel-translatable-plugin": "^2.0",
        "filament/tables": "^2.16",
        "firebase/php-jwt": "^5.4",
        "fruitcake/laravel-cors": "^2.0",
        "genl/matice": "^1.1",
        "guzzlehttp/guzzle": "^7.5",
        "inertiajs/inertia-laravel": "^0.6.4",
        "intervention/image": "^2.7.2",
        "jumbojett/openid-connect-php": "^0.9.2",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.7",
        "laravel/octane": "^v1.3.6",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.5",
        "mtvs/eloquent-hashids": "^3.3",
        "ohseesoftware/laravel-vite-manifest": "^1.2",
        "sentry/sentry-laravel": "^2.8",
        "spatie/laravel-permission": "^5.4.2",
        "spatie/laravel-query-builder": "^5.1",
        "spatie/laravel-translatable": "^5.2",
        "tightenco/ziggy": "^1.0",
        "vinkla/hashids": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "jasonmccreary/laravel-test-assertions": "^1.0",
        "laracasts/generators": "^2.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan horizon:install --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
